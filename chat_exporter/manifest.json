{
  "manifest_version": 3,
  "name": "Google Scholar Labs Exporter",
  "version": "1.0",
  "description": "Export Google Scholar Labs conversations to markdown",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Export Conversation"
  },
  "host_permissions": [
    "https://scholar.google.com/*"
  ]
}